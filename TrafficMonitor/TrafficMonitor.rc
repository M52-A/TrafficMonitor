BEGIN
    POPUP "選單"
    BEGIN
        MENUITEM "複製文字(&C)",                    ID_COPY_TEXT
    END
END

IDR_MENU1 MENU
BEGIN
    POPUP "選單"
    BEGIN
        POPUP "選擇網路連線(&S)"
        BEGIN
            MENUITEM "自動選擇(&A)",                    ID_SELETE_CONNECTION
            MENUITEM "選擇全部(&S)",                    ID_SELECT_ALL_CONNECTION
        END
        MENUITEM "連線細節(&I)",                    ID_NETWORK_INFO
        MENUITEM SEPARATOR
        MENUITEM "最上層顯示(&T)",                   ID_ALWAYS_ON_TOP
        MENUITEM "游標穿透(&M)",                    ID_MOUSE_PENETRATE
        MENUITEM "鎖定視窗位置(&L)",                  ID_LOCK_WINDOW_POS
        MENUITEM "顯示更多資訊(&C)",                  ID_SHOW_CPU_MEMORY
        MENUITEM "顯示工作列視窗(&W)",                 ID_SHOW_TASK_BAR_WND
        MENUITEM "顯示主視窗(&H)",                   ID_SHOW_MAIN_WND
        POPUP "視窗不透明度(&P)"
        BEGIN
            MENUITEM "100%",                        ID_TRANSPARENCY_100
            MENUITEM "80%",                         ID_TRANSPARENCY_80
            MENUITEM "60%",                         ID_TRANSPARENCY_60
            MENUITEM "40%",                         ID_TRANSPARENCY_40
        END
        MENUITEM SEPARATOR
        POPUP "其他功能(&E)"
        BEGIN
            MENUITEM "更換面板(&S)...",                 ID_CHANGE_SKIN
            MENUITEM "更換通知區域圖示(&N)...",             ID_CHANGE_NOTIFY_ICON
            MENUITEM SEPARATOR
            MENUITEM "允許超出螢幕邊界(&B)",                ID_ALOW_OUT_OF_BORDER
            MENUITEM SEPARATOR
            MENUITEM "歷史流量統計(&H)",                  ID_TRAFFIC_HISTORY
            MENUITEM "外掛管理(&P)...",                 ID_PLUGIN_MANAGE
        END
        MENUITEM "選項(&O)...",                   ID_OPTIONS
        MENUITEM SEPARATOR
        POPUP "說明(&H)"
        BEGIN
            MENUITEM "說明(&H)",                      ID_HELP
            MENUITEM "常見問題(&F)",                    ID_FREQUENTY_ASKED_QUESTIONS
            MENUITEM "更新記錄(&L)",                    ID_UPDATE_LOG
            MENUITEM "關於(&A)...",                   ID_APP_ABOUT
            MENUITEM "檢查更新(&C)...",                 ID_CHECK_UPDATE
        END
        MENUITEM "結束(&X)",                      ID_APP_EXIT
    END
END

