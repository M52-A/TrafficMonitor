IDR_HISTORY_TRAFFIC_MENU MENU
BEGIN
    POPUP "選單"
    BEGIN
        POPUP "顯示比例"
        BEGIN
            MENUITEM "使用線性比例",                      ID_USE_LINEAR_SCALE
            MENUITEM "使用對數比例",                      ID_USE_LOG_SCALE
        END
    END
    POPUP "2"
    BEGIN
        POPUP "一週的第一天"
        BEGIN
            MENUITEM "星期日",                         ID_FIRST_DAY_OF_WEEK_SUNDAY
            MENUITEM "星期一",                         ID_FIRST_DAY_OF_WEEK_MONDAY
        END
        MENUITEM "轉到今天",                        ID_CALENDAR_JUMP_TO_TODAY
    END
END

